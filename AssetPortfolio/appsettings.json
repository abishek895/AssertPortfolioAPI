{
  "ConnectionStrings": {
    "AssetPortfolioDB": "Server=(localdb)\\MSSQLLocalDB;Database=AssetPortfolioDB;TrustServerCertificate=true;Integrated Security=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
    
  }
}
